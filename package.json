{
  "name": "sentry",
  "author": "woodi",
  "license": "MIT",
  "version": "0.1.0",
  "homepage": "https://github.com/woodi97/sentry",
  "description": "frontend boilerplate support component based dev",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woodi97/sentry.git"
  },
  "scripts": {
    "prepare": "husky install",
    "tsc:w": "tsc -w --outDir ./public/js",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "build:dev": "cross-env NODE_ENV=development webpack --mode=development",
    "start": "cross-env NODE_ENV=production webpack serve --mode=production",
    "start:dev": "cross-env NODE_ENV=development webpack server --mode=development"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "lodash-es": "^4.17.21",
    "peerjs": "^1.3.2",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
